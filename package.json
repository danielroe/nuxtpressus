{
  "name": "nuxtpressus",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": {
    "name": "Daniel Roe",
    "email": "daniel@roe.dev",
    "url": "https://roe.dev"
  },
  "scripts": {
    "build": "nuxt build --preset netlify-static",
    "dev": "nuxt dev",
    "lint": "eslint .",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && simple-git-hooks",
    "test:types": "vue-tsc --noEmit",
    "test:unit": "vitest",
    "test:nuxt": "vitest -c vitest.nuxt.config.ts",
    "test:browser": "playwright test test/browser",
    "test:browser:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.49.1-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/browser --update-snapshots'"
  },
  "dependencies": {
    "@directus/sdk": "^18.0.3",
    "@nuxt/eslint": "^1.0.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.8.1",
    "@nuxt/scripts": "^0.9.5",
    "@nuxt/test-utils": "3.15.1",
    "@nuxtjs/html-validator": "^2.0.0",
    "@unocss/nuxt": "^66.0.0",
    "directus-sdk-typegen": "0.1.7",
    "nuxt": "^3.14.1592",
    "nuxt-og-image": "^5.0.0",
    "nuxt-rebundle": "^0.0.2",
    "nuxt-time": "^1.0.3",
    "ufo": "^1.5.4",
    "unocss": "^66.0.0",
    "unplugin-vue-router": "^0.10.9"
  },
  "devDependencies": {
    "@playwright/test": "1.49.1",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.22.0",
    "happy-dom": "15.11.7",
    "lint-staged": "15.2.11",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.6.3",
    "vitest": "2.1.9",
    "vue-tsc": "2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.15.0"
}
